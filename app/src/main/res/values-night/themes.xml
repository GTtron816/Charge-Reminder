<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ChargingStopReminder" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/monetbuttonb</item>
        <item name="colorPrimaryVariant">@color/monetshadeb</item>
        <item name="colorOnPrimary">@color/monetbgb</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/monetbuttonb</item>
        <item name="colorSecondaryVariant">@color/moneticonb</item>
        <item name="colorOnSecondary">@color/monetbgb</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->

        <item name="android:textColor">@color/moneticonb</item>
    </style>

    <style name="cardbg">
        <item name="android:background">@color/monetshadeb</item>

    </style>

    <style name="sw">
        <item name="android:textColor">@color/moneticonb</item>
        <item name="android:textSize">16sp</item>
    </style>

</resources>

